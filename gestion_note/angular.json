{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "projects": {
    "gestion_note": {
      "architect": {
        "build": {
          "options": {
            "styles": [
              "gestion_note/src/styles.css"
            ],
            "scripts": []
          }
        },
        "server": {
          "options": {
            "main": "gestion_note/src/main.server.ts",
            "outputPath": "dist/gestion_note-server",
            "tsConfig": "gestion_note/tsconfig.server.json"
          }
        },
        "prerender": {
          "options": {
            "main": "gestion_note/src/main.server.ts",
            "tsConfig": "gestion_note/tsconfig.server.json",
            "routes": [
              "/"
            ]
          }
        },
        "ssr": {
          "options": {
            "entry": "../gestion_note_server/server.ts"
          }
        }
      }
    }
  }
}